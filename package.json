{
  "name": "react-virtuoso",
  "author": "Petyo Ivanov",
  "type": "module",
  "sideEffects": false,
  "version": "0.0.0-development",
  "homepage": "https://virtuoso.dev/",
  "license": "MIT",
  "source": "src/index.tsx",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "react",
    "reactjs",
    "virtual",
    "endless",
    "load more",
    "list",
    "scrolling",
    "infinite",
    "virtualized",
    "grid",
    "feed",
    "chat"
