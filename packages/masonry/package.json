{
  "name": "@virtuoso.dev/masonry",
  "private": false,
  "description": "Virtualized React component for rendering masonry layouts",
  "keywords": [
    "react",
    "virtual",
    "masonry",
    "endless scrolling",
    "list",
    "scrolling",
    "infinite",
