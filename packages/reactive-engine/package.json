{
  "name": "@virtuoso.dev/reactive-engine",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "test": "vitest --run --browser.headless --typecheck",
    "test:watch": "vitest --browser.headless --typecheck",
    "test:browser": "vitest",
    "build": "tsc -b && vite build",
    "lint": "eslint",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "typecheck": "tsgo --noEmit",
    "start:example": "cd example && vite --config ./vite.config.ts",
    "start:example:todo": "cd example-todo-app && vite --config ./vite.config.ts",
    "start:example:todo:api": "json-server example-todo-app/db.json --port 3001"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@virtuoso.dev/tooling": "workspace:*",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/node": "^22.10.1",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/browser": "^4.0.16",
    "eslint": "^9.19.0",
    "jsdom": "^26.0.0",
    "globals": "^16.4.0",
    "prettier": "^3.4.2",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "json-server": "1.0.0-beta.3",
    "markdownlint-cli2": "^0.18.1",
    "playwright": "^1.33.0",
    "typescript": "~5.9.3",
    "vite": "^6.4.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.16",
    "vitest-browser-react": "^2.0.2"
  }
}
