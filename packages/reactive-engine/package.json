{
  "name": "@virtuoso.dev/reactive-engine",
  "version": "0.1.0",
  "description": "Reactive state management for complex web applications.",
  "author": "Petyo Ivanov",
  "homepage": "https://virtuoso.dev",
  "bugs": {
    "url": "https://github.com/petyosi/react-virtuoso/issues"
  },
  "keywords": [
    "react",
    "state-management",
    "reactive",
    "push-based",
    "performance",
    "state"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsc && vite build",
    "test": "vitest --run --browser.headless",
    "test:watch": "vitest --browser.headless",
    "ci-lint": "eslint",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "ladle": "ladle serve",
    "are-the-types-wrong": "attw --profile esm-only --pack ."
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/petyosi/react-virtuoso.git",
    "directory": "packages/reactive-engine"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": ">= 16 || >= 17 || >= 18 || >= 19",
    "react-dom": ">= 16 || >= 17 || >= 18 || >= 19"
  },
  "devDependencies": {
    "@virtuoso.dev/tooling": "*"
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ]
}
