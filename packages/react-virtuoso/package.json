    "@eslint/js": "^9.19.0",
    "@faker-js/faker": "^7.6.0",
    "@microsoft/api-extractor": "^7.55.1",
    "@ladle/react": "^5.0.1",
    "@playwright/test": "^1.33.0",
    "@testing-library/react": "^16.3.0",
    "@types/jsdom": "^21.1.1",
    "@types/lodash": "^4.14.194",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.19.0",
